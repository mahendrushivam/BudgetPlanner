<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:startColor="#9f526b"
                android:centerColor="#aac6c6"
                android:centerY="0.75"
                android:endColor="#d9266f"
                android:angle="270"
                />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="#800059ff"
                    android:centerColor="#800059ff"
                    android:centerY="0.75"
                    android:endColor="#800059ff"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>
    <item
        android:id="@android:id/progress"
        >
        <clip>
            <shape>
                <corners
                    android:radius="5dip" />
                <gradient
                    android:startColor="#b1bd3f49"
                    android:endColor="#79afb8"
                    android:angle="270" />
            </shape>
        </clip>
    </item>

</layer-list>